{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug math.exe",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/math.exe",
        "cwd": "${workspaceFolder}",
        "MIMode": "gdb",
        "miDebuggerPath": "C:/mingw64/bin/gdb.exe",
        "setupCommands": [
          {
            "description": "Enable pretty-printing",
            "text": "-enable-pretty-printing"
          }
        ],
        "preLaunchTask": "build-and-link"
      }
    ]
  }